{
  "name": "lets-js",
  "version": "0.0.0",
  "homepage": "https://github.com/openlattice/lets-js",
  "description": "a bootstrapped project to serve as a starting point for a new JavaScript library",
  "author": {
    "name": "OpenLattice, Inc.",
    "email": "support@openlattice.com",
    "url": "https://openlattice.com"
  },
  "contributors": [
    {
      "name": "Hristo Oskov",
      "email": "hristo@openlattice.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openlattice/lets-js.git"
  },
  "bugs": {
    "url": "https://github.com/openlattice/lets-js/issues"
  },
  "license": "Apache-2.0",
  "main": "build/lib.js",
  "dependencies": {
    "immutable": "4.0.0-rc.9",
    "lodash": "~4.17.10",
    "loglevel": "~1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.5.5",
    "@babel/core": "~7.5.5",
    "@babel/plugin-proposal-class-properties": "~7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "~7.5.0",
    "@babel/plugin-transform-runtime": "~7.5.0",
    "@babel/polyfill": "~7.4.0",
    "@babel/preset-env": "~7.5.0",
    "@babel/preset-flow": "~7.0.0",
    "@babel/register": "~7.4.0",
    "@babel/runtime": "~7.5.0",
    "babel-eslint": "~10.0.0",
    "babel-jest": "~24.8.0",
    "babel-loader": "~8.0.0",
    "eslint": "~6.0.0",
    "eslint-config-airbnb-base": "~13.2.0",
    "eslint-import-resolver-webpack": "~0.11.0",
    "eslint-loader": "~2.2.0",
    "eslint-plugin-flowtype": "~3.11.0",
    "eslint-plugin-import": "~2.18.0",
    "eslint-plugin-jest": "~22.10.0",
    "flow-bin": "~0.102.0",
    "jest": "~24.8.0",
    "npm-run-all": "~4.1.0",
    "rimraf": "~2.6.0",
    "webpack": "~4.36.0",
    "webpack-cli": "~3.3.0",
    "webpack-dev-server": "~3.7.0"
  },
  "peerDependencies": {
    "@babel/polyfill": "~7.4.0",
    "@babel/runtime": "~7.5.0"
  },
  "scripts": {
    "build": "webpack --config config/webpack/webpack.config.js --env.production",
    "clean": "rimraf build/*",
    "flow": "flow check",
    "lib": "webpack-dev-server --config config/webpack/webpack.config.js --env.development --port 9000 --content-base build/",
    "lint": "eslint --config .eslintrc src",
    "release:major": "npm version major -m \"releasing v%s\" && npm publish",
    "release:minor": "npm version minor -m \"releasing v%s\" && npm publish",
    "release:patch": "npm version patch -m \"releasing v%s\" && npm publish",
    "preversion": "npm-run-all lint test",
    "version": "npm-run-all clean build && git add -A package.json build/",
    "postversion": "git push && git push --tags",
    "test": "jest --config config/jest/jest.config.js",
    "test:travis": "jest --maxWorkers=4 --config config/jest/jest.config.js",
    "test:watch": "jest --config config/jest/jest.config.js --watch --coverage=false"
  }
}
